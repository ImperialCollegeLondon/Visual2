{
  "name": "visual",
  "productName": "VisUAL2 Easter 18",
  "main": "main.js",
  "scripts": {
    "start": "cd src/Main && dotnet fable webpack --port free -- -w --config webpack.config.js",
    "build": "cd src/Main && dotnet fable webpack --port free -- --config webpack.config.js",
    "package": "yarn run build && electron-packager . visual2 --arch=x64 --platform=darwin,linux,win32 --out=dist --prune --asar --ignore=/dist --ignore=/src --overwrite",
    "package-host": "yarn run build && electron-packager . visual2 --out=dist --prune --asar --ignore=/dist --ignore=/src --overwrite",
    "package-osx": "yarn run build && electron-packager . visual2 --platform=darwin --arch=x64 --out=dist --prune --asar --ignore=/dist --ignore=/src --overwrite",
    "package-linux": "yarn run build && electron-packager . visual2 --platform=linux --arch=x64 --out=dist --prune --asar --ignore=/dist --ignore=/src --overwrite",
    "package-win": "yarn run build && electron-packager . visual2 --platform=win32 --arch=x64 --out=dist --prune --asar --ignore=/dist --ignore=/src --overwrite --icon app/visual.ico",
    "launch": "electron .",
    "make-osx-dmg": "yarn run package-osx && electron-installer-dmg --overwrite dist/visual2-darwin-x64 dist/visual2-osx --icon app/visual.ico"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "copyfiles": "^2.0.0",
    "electron-context-menu": "^0.9.1",
    "electron-settings": "^3.2.0",
    "electron-tabs": "^0.9.1",
    "monaco-editor": "^0.12.0",
    "requirejs": "^2.3.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-standalone": "^6.26.0",
    "babel-template": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "electron": "1.8.7",
    "electron-installer-dmg": "^1.0.0",
    "electron-packager": "^12.0.0",
    "fable-loader": "^1.0.7",
    "fable-utils": "^1.0.2",
    "loglevel": "^1.4.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.11.0"
  }
}
