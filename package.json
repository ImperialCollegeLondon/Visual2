    "requirejs": "^2.3.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-standalone": "^6.26.0",
    "babel-template": "^6.26.0",
    "electron": "^1.7.5",
    "electron-packager": "^12.0.0",
    "fable-loader": "^1.0.7",
    "fable-utils": "^1.0.2",
    "loglevel": "^1.4.1",
